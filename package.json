{
  "name": "@ganbarodigital/ts-lib-apperror",
  "version": "0.0.1",
  "description": "Structured errors for easier reporting and handling",
  "repository": "github:ganbarodigital/ts-lib-apperror",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "lint": "tslint -p tsconfig.json",
    "prepublishOnly": "npm test && npm run lint",
    "prepack": "npm run build",
    "test": "npm run clean && tsc && mocha -r ts-node/register \"src/**/*.spec.ts\"",
    "cover": "rimraf coverage && rimraf .nyc_output && nyc --cache=false npm run test",
    "clean": "rimraf lib"
  },
  "contributors": [
    "Stuart Herbert <stuherbert@ganbarodigital.com> (https://ganbarodigital.com)"
  ],
  "keywords": [
    "errors",
    "logging"
  ],
  "files": [
    "lib/**/*",
    "CHANGELOG.md",
    "LICENSE.md",
    "V1/**"
  ],
  "license": "BSD-3-Clause",
  "dependencies": {
    "@ganbarodigital/ts-lib-packagename": "^0.1.0",
    "@ganbarodigital/ts-lib-value-objects": "file:../ts-lib-value-objects",
    "@types/node": "^13.1.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "@types/chai": "^4.2.8",
    "@types/mocha": "^7.0.1",
    "chai": "^4.2.0",
    "chai-bytes": "^0.1.2",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.0",
    "ts-node": "^8.0.0"
  }
}
